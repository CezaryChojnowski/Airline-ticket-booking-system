<Window x:Class="WPFClient.AntiBotCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClient"
        mc:Ignorable="d"
        Title="AntyBot" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image Source="Images/code.png" HorizontalAlignment="Left" Height="182" Margin="135,34,0,0" VerticalAlignment="Top" Width="606"/>
        <Label Content="Przepisz kod z obrazka:" FontWeight="Bold" FontSize="25" HorizontalAlignment="Left" Margin="98,259,0,0" VerticalAlignment="Top"/>
        <TextBox Name="code" HorizontalAlignment="Left" FontSize="25" Height="42" Margin="403,259,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="276"/>
        <Button Name="CheckCode" Content="Dalej" HorizontalAlignment="Left" Margin="286,341,0,0" VerticalAlignment="Top" Width="221" Height="42" Click="CheckCode_Click"/>

    </Grid>
</Window>
